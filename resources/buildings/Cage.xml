<?xml version="1.0" ?>

<building name="Cage" game_type="Cage"> 
	<if>
		<MaterialType value="Wood" />
		<sprite index="313" />
	</if>
	<else>
		<MaterialType value="GreenGlass"/>
		<sprite index="314" />
	</else>
	<else>
		<or>
			<MaterialType value="ClearGlass"/>
			<MaterialType value="CrystalGlass"/>
		</or>
		<sprite index="294" />
	</else>
	<else>
		<sprite index="312" color="building" />
	</else>

</building> 
